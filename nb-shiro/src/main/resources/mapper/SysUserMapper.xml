<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nb.shiro.mapper.SysUserMapper">


    <select id="findByUsername" parameterType="String" resultType="com.nb.shiro.entity.SysUser">
      select * from sys_user where username = #{username} and del_flag=0 limit 1
    </select>
</mapper>
